import java.util.Scanner;
class Github3
{
	public static void main(String args[])
	{
		Scanner sc=new Scanner(System.in);
		String s=sc.nextLine();
		int l=s.length();
		for(int x=0;x<=l-1;x++)
		{
			char ch=s.charAt(x);
			if(ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u'||ch=='A'||ch=='E'||ch=='I'||ch=='O'||ch=='U')
			{
				s=s.replace(ch,'*');
			}
		}
		System.out.println(s);
	}
}
